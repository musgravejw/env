[alias]
	s = status
	b = branch
	c = checkout
	ca = commit -m
	lp = log -15 --pretty=oneline
	ri = rebase -i HEAD~2
	lg = log --graph --full-history --all --color --pretty=tformat:"%x1b[31m%h%x09%x1b[32m%d%x1b[0m%x20%s%x20%x1b[33m(%an)%x1b[0m"
[color]
	ui = auto
[core]
	editor = /usr/bin/vim
